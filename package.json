{
  "name": "pushpin",
  "productName": "pushpin",
  "version": "1.0.0",
  "description": "A collaborative corkboard app",
  "repository": "https://github.com/inkandswitch/pushpin",
  "main": "src/electron.js",
  "scripts": {
    "start": "electron-forge start",
    "start2": "NAME=userA npm start & NAME=userB npm start",
    "start3": "NAME=userA npm start & NAME=userB npm start & NAME=userC npm start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "test": "electron-mocha --renderer test/**/*.{js,jsx}",
    "build-docs": "rm -rf ./docs && parcel build src/docs.html -d docs -o index --public-url '.' && rm -rf .cache"
  },
  "keywords": [],
  "author": "inkandswitch",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "pushpin"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "inkandswitch",
        "name": "pushpin"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "pushpin"
      }
    }
  },
  "dependencies": {
    "@ibm/plex": "^1.4.1",
    "automerge": "^0.10.1",
    "bs58": "^4.0.1",
    "classnames": "^2.2.6",
    "codemirror": "^5.47.0",
    "diff-match-patch": "^1.0.4",
    "discovery-cloud-client": "0.0.1",
    "discovery-swarm": "^5.1.4",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-prompt": "^1.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "hypercore": "^7.2.0",
    "hypercore-protocol": "^6.11.0",
    "hyperdiscovery": "^9.0.1",
    "hypermerge": "github:inkandswitch/hypermerge",
    "immutable": "^3.8.2",
    "jimp": "^0.6.4",
    "js-crc": "^0.2.0",
    "line-awesome": "git+https://github.com/choxi/line-awesome.git",
    "mkdirp": "0.5.1",
    "react": "16.8.6",
    "react-color": "^2.17.3",
    "react-contextmenu": "2.11.0",
    "react-dom": ">=16.8.6",
    "react-hot-loader": "^4.8.8",
    "react-measure": "^2.3.0",
    "react-rnd": "^9.1.2",
    "react-simple-dropdown": "^3.2.3",
    "riek": "^1.1.0",
    "tmp": "0.1.0",
    "to-buffer": "^1.1.1",
    "unfluff": "^3.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "electron-forge": "^5.2.4",
    "electron-mocha": "^8.0.2",
    "electron-prebuilt-compile": "4.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-15": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "mocha": "^6.1.4",
    "parcel-bundler": ">=1.12.3",
    "react-test-renderer": "^16.8.6"
  }
}
