  "scripts": {
    "build-deps": "electron-builder install-app-deps",
    "compile": "webpack",
    "start": "webpack-dev-server --watch",
    "start2": "NAME=userA yarn start & NAME=userB yarn start",
    "start3": "NAME=userA yarn start & NAME=userB yarn start & NAME=userC yarn start",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "lint": "eslint --cache --color --ext .jsx,.js,.tsx,.ts src",
    "test": "electron-mocha --renderer test/**/*.{js,jsx,ts,tsx}",
    "build-docs": "rm -rf ./docs && parcel build src/docs.html -d docs -o index --public-url '.' && rm -rf .cache",
    "postinstall": "electron-builder install-app-deps"
  },
