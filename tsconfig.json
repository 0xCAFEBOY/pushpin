{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "noFallthroughCasesInSwitch": true,
    "lib": [
      "es2015",
      "es2017",
      "dom",
      "webworker"
    ],
    "esModuleInterop": true,
    // "module": "commonjs", // Handled by tsconfig-base
    // "target": "es2015", // Handled by tsconfig-base
    "jsx": "react",
    "pretty": true
  }
}
